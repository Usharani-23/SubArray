# SubArray
public class SubArray {
public static void main(String[] args) {
int arr[] = {1,2,3,4 };
for (int i = 0; i < arr.length; i++)
   for (int j = 0; j < arr.length-i; j++) {
       for (int k = i; k <=i+j; k++)

            System.out.print(arr[k] + " ");
    System.out.println(" ");
      }
     }
    
   }


    



      



     

      


      

      



      

      


           


     

  

   
